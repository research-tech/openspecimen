<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

  <changeSet author="ckhilosiya" id="Enabling or Disabling unique specimen label per CP">
    <sql>
      call add_cfg_prop(
        'biospecimen', 'unique_spmn_label_per_cp',
        'unique_spmn_label_per_cp', 'unique_spmn_label_per_cp_desc',
        'BOOLEAN', 'false'
      );
    </sql>
  </changeSet>

  <changeSet author="slakhani" id="Config setting to specify OS deploy environment">
    <sql>
      call add_cfg_prop(
        'common', 'deploy_env',
        'deploy_env', 'deploy_env_desc',
        'STRING', 'TEST'
      );
    </sql>
  </changeSet>
</databaseChangeLog>